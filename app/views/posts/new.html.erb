
<p>
  <a class="btn btn-success btn-block text-left" data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
    Create Lap Time
  </a>
</p>
<div class="collapse" id="collapseExample">
  <div class="card card-body">
  <%= form_for(@post, :method => 'post') do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f})%>
    <div>
      <%= f.submit('Add Time')%>
    </div>
  <% end %>
  <%= link_to('Back to Posts', posts_path) %>


  </div>
</div>
